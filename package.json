{
  "name": "electrojector",
  "version": "0.0.1",
  "description": "Simple dependency injection for JS",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "ava": "~1.2.0",
    "standard": "~12.0.1"
  },
  "dependencies": {
